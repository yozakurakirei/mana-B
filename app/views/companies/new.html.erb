<h1>クライアント登録</h1>

<%= form_with(model: @company, local: true) do |f| %>
  <%= f.label :name, "会社名" %>
  <%= f.text_field :name %>

  <%= f.label :name_kana, "会社名" %>
  <%= f.text_field :name_kana %>

  <%= f.label :product, "商品" %>
  <%= f.text_field :product %>

  <%= f.label :brand, "ブランド" %>
  <%= f.text_field :brand %>

  <%= f.submit "登録" %>
<% end %>
